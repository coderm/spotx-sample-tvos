FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/SpotX-SDK-AVPlayer"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/SpotX-SDK-AVPlayer"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/SpotX-SDK-AVPlayer"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "AdSupport" -framework "CoreLocation" -framework "Security" -framework "SpotX" -framework "SystemConfiguration" -framework "UIKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
